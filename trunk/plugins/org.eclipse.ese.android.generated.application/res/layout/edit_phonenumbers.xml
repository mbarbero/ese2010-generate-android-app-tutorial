<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout 
	android:id="@+id/layout"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">

	<TextView 
		android:id="@+id/wCellPhoneLabel"
		android:layout_width="wrap_content"
		android:layout_height="40px"
		android:text="CellPhone"
		android:layout_x="20px"
		android:layout_y="28px">
	</TextView>
	<EditText 
		android:id="@+id/wCellPhone"
		android:layout_width="120px"
		android:layout_height="40px"
		android:text=""
		android:textSize="18sp" 
		android:layout_x="180px"
		android:layout_y="20px">
	</EditText>
	
	<TextView 
		android:id="@+id/wWorkPhoneLabel"
		android:layout_width="wrap_content"
		android:layout_height="40px"
		android:text="WorkPhone"
		android:layout_x="20px"
		android:layout_y="68px">
	</TextView>
	<EditText 
		android:id="@+id/wWorkPhone"
		android:layout_width="120px"
		android:layout_height="40px"
		android:text=""
		android:textSize="18sp" 
		android:layout_x="180px"
		android:layout_y="60px">
	</EditText>
	
	
	<Button
		android:id="@+id/wBackPhoneNumbers"
		android:layout_width="280px"
		android:layout_height="40px"
		android:text="Back"
		android:layout_x="20px"
		android:layout_y="110px">
	</Button>	
	
</AbsoluteLayout>
